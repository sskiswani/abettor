{
  "extends": "tslint:recommended",
  "jsRules": {
    "comment-format": [
      false
    ],
    "forin": false,
    "jsdoc-format": true,
    "max-line-length": [
      130
    ],
    "member-access": [
      false
    ],
    "no-bitwise": false,
    "no-consecutive-blank-lines": [
      true,
      2
    ],
    "no-console": [
      false
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": [
      true
    ],
    "prefer-for-of": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "trailing-comma": [
      false
    ],
    "variable-name": [
      true,
      "allow-leading-underscore",
      "allow-pascal-case",
      "ban-keywords"
    ],
    "whitespace": [
      "check-branch",
      "check-decl",
      "check-module",
      "check-operator",
      "check-separator",
      "check-type"
    ]
  },
